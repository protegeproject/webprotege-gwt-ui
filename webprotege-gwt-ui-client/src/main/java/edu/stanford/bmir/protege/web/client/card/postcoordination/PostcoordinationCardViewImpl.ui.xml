<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="pc" type="edu.stanford.bmir.protege.web.client.postcoordination.PostCoordinationTableResourceBundle"/>
    <g:HTMLPanel ui:field="paneContainer">
        <g:HTMLPanel width="100%" addStyleNames="{pc.style.horizontalScrollContainer}">
            <g:ScrollPanel width="100%" height="100%" addStyleNames="{pc.style.horizontalScrollPanel}">
                <g:FlexTable width="97%" height="100%" ui:field="flexTable"/>
            </g:ScrollPanel>
        </g:HTMLPanel>
        <g:VerticalPanel width="100%" ui:field="scaleValueCardList"/>
    </g:HTMLPanel>

</ui:UiBinder>