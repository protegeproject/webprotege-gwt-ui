<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>

    <ui:style>
        .headerBar {
            margin-bottom: 2em;
        }
    </ui:style>
    <g:HTMLPanel>
        <g:HTMLPanel addStyleNames="{style.headerBar}">
            <g:Button ui:field="resetRolesButton" text="Reset project roles" addStyleNames="{wp.buttons.button} {wp.buttons.smallPageButton} {wp.buttons.secondaryButton}"/>
        </g:HTMLPanel>
        <g:SimplePanel ui:field="container"/>
    </g:HTMLPanel>
</ui:UiBinder>