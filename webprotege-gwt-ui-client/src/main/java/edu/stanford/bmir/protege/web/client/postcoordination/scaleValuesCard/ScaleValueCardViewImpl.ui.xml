<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>
    <ui:with field="pc"
             type="edu.stanford.bmir.protege.web.client.postcoordination.PostCoordinationTableResourceBundle"/>
    <g:HTMLPanel>
        <g:HorizontalPanel>
            <g:VerticalPanel>
                <g:Label ui:field="titleLabel" styleName="cm-strong cm-def"/>
                <g:Button ui:field="addButton" text="+ Select value" styleName="cm-link"/>
            </g:VerticalPanel>
            <g:FlexTable width="97%" height="100%" ui:field="valueTable"
                         styleName="{pc.style.getPostCoordinationTable}"/>
        </g:HorizontalPanel>
    </g:HTMLPanel>

</ui:UiBinder>
