<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:graphtree="urn:import:edu.stanford.protege.gwt.graphtree.client">
    <ui:style>
        .main {
            display: flex;
            flex-direction: column;
            width: 500px;
            height: 300px;
            overflow: auto;
        }
    </ui:style>
    <g:HTMLPanel ui:field="main" addStyleNames="{style.main}">
        <graphtree:TreeWidget ui:field="treeWidget"/>
    </g:HTMLPanel>
</ui:UiBinder>