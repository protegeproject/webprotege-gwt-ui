<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .main {
            display: flex;
            flex-direction: row;
        }
    </ui:style>
    <g:HTMLPanel addStyleNames="{style.main}">
        <g:ListBox ui:field="typeIdSelector"/>
        <g:SimplePanel ui:field="presenterContainer"/>
    </g:HTMLPanel>
</ui:UiBinder>